{
  "name": "gdg-on-campus-portal",
  "type": "module",
  "version": "0.0.0",
  "description": "GDG on Campus-Portal",
  "main": "app.js",
  "scripts": {
    "dev:client": "cd client && npm run dev",
    "dev:client:docker": "cd client && npm run dev:docker", 
    "dev:server": "cd server && npm run dev:win",
    "dev:server:docker": "cd server && npm run dev:docker:win",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:docker": "concurrently \"npm run dev:server:docker\" \"npm run dev:client:docker\"",
    "build": "cd client && npm run build",
    "build:docker": "cd client && npm run build:docker",
    "build:dev": "cd client && npm run build:dev",
    "start": "cd server && npm start",
    "start:docker": "cd server && npm run start:docker:win",
    "install:all": "npm install && cd client && npm install && cd ../server && npm install",
    "db:setup": "./scripts/setup-database.sh",
    "db:init": "node scripts/init-database.js",
    "db:clear": "node scripts/init-database.js --clear",
    "db:check": "node scripts/check-database.js",
    "db:generate-sql": "node scripts/generate-sql.js",
    "test:all": "./tests/run-all-tests.sh"
  },
  "author": {
    "name": ""
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1"
  },
  "eslintConfig": {},
  "dependencies": {
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "framer-motion": "^12.9.1",
    "gdg-on-campus-portal": "file:",
    "node-fetch": "^3.3.2"
  }
}
